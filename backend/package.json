{
  "name": "mokuen-swapforest-backend",
  "version": "1.0.0",
  "description": "Backend API for Mokuen SwapForest - A Web3 forest ecosystem where user activity makes trees grow",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "node dist/main",
    "start:dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "npm run build",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:prod": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "db:seed": "ts-node prisma/seed.ts",
    "db:reset": "prisma migrate reset --force",
    "db:reset:seed": "npm run db:reset && npm run db:seed"
  },
  "keywords": [
    "nestjs",
    "web3",
    "wallet-auth",
    "neon-database",
    "postgresql",
    "swagger",
    "forest-ecosystem",
    "activity-based-growth"
  ],
  "author": "Mokuen Team",
  "license": "MIT",
  "dependencies": {
    "@nestjs/cli": "^11.0.7",
    "@nestjs/common": "^11.1.5",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^11.1.5",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^11.1.5",
    "@nestjs/swagger": "^7.4.2",
    "@nestjs/throttler": "^6.2.1",
    "@prisma/client": "^6.12.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "dotenv": "^17.2.1",
    "ethers": "^5.7.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "prisma": "^6.12.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@types/node": "^24.1.0",
    "@types/passport-jwt": "^4.0.1",
    "@types/swagger-ui-express": "^4.1.6",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}